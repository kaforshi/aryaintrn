# Cloudflare Tunnel Configuration untuk aryaintaran.dev
# Copy file ini ke /etc/cloudflared/config.yml
# Ganti YOUR_TUNNEL_ID dengan Tunnel ID dari Cloudflare Dashboard

tunnel: YOUR_TUNNEL_ID
credentials-file: /etc/cloudflared/credentials.json

ingress:
  # Route untuk aplikasi Laravel (root domain)
  - hostname: aryaintaran.dev
    service: http://127.0.0.1:8000
  
  # Route untuk www subdomain (optional)
  - hostname: www.aryaintaran.dev
    service: http://127.0.0.1:8000
  
  # Catch-all rule (harus di akhir)
  - service: http_status:404

